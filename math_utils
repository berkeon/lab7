
def add(x, y):
    return x + y

def subtract(x, y):
    return x - y

def multiply(x, y):
    return x * y

def divide(x, y):
    try:
        return x / y
    except ZeroDivisionError:
        return "error! division by zero not allowed"

def power(x, y):
    return x ** y

def mod(x, y):
    return x % y

if __name__ == "__main__":
    print(add(10, 5))
    print(subtract(20,6))
    print(multiply(22,4))
    print(divide(20,5))
    print(power(2,5))
    print(mod(26,7))

